!use post
!set outputformat psql
!if (false) {
select *, first_value(deptno) over () from emp;
 ename | deptno | gender | first_value
-------+--------+--------+-------------
 Jane  |     10 | F      |          10
 Bob   |     10 | M      |          10
 Eric  |     20 | M      |          10
 Susan |     30 | F      |          10
 Alice |     30 | F      |          10
 Adam  |     50 | M      |          10
 Eve   |     50 | F      |          10
 Grace |     60 | F      |          10
(8 rows)

!ok
!}
!if (false) {
select *, first_value(ename) over () from emp;
 ename | deptno | gender | first_value
